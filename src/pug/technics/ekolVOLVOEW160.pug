
mixin tech(name)
  div(id="" + name class="tech")

mixin ident(identifikator)
  div(id="" + identifikator class="card-body")

- var subPage= 'Экскаватор колёсный VOLVO EW 160', value = "Спецтехника", name = 'ekolVOLVOEW160', identifikator = 'ekskKoles', modalHidden = true;
- var descr = 'Аренда колесного экскаватора VOLVO EW 160 в Санкт-Петербурге на отличных условиях, аренда за смену от 13600 руб. Стоимость аренды колесного экскаватора может быть снижена на индивидуальных условиях.';
- var image = 'https://terex-plus.ru/build/img/' + name + '.png';



doctype html
html(lang="ru")
    include ../head
    body
      include ../header
      include ../strip
      include ../technic
      include ../footer
      script(src="../build/js/all.js")
