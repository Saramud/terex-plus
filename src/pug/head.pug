    {
        "@context": "http://schema.org",
        "@type": "Organization",
        "name": "Терекс-Плюс",
        "url": "https://terex-plus.ru",
        "logo": "https://terex-plus.ru/build/img/logo_3.webp",
        "contactPoint": [
            {
                "@type": "ContactPoint",
                "telephone": "+7(812)989-03-32",
                "contactType": "customer service",
                "email": "info@terex-plus.ru",
                "availableLanguage": [
                    "Русский"
                ]
            }
        ]
    }
  
  if (page != 'about' && page != 'contacts')
