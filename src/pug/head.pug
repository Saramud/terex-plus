  script(type='application/ld+json').
    {
      "@context": "http://schema.org",
      "@type": "LocalBusiness",
      "name": "Терекс-Плюс",
      "image": "https://terex-plus.ru/build/img/logo_3.webp",
      "url": "https://terex-plus.ru",
      "telephone": "+7(812)989-03-32",
      "address": {
        "@type": "PostalAddress",
        "streetAddress": "ТАЛЛИНСКОЕ ШОССЕ, 153/3",
        "addressLocality": "Санкт-Петербург",
        "addressCountry": "RU"
      },
      "geo": {
      "@type": "GeoCoordinates",
      "latitude": "59.8124485",
      "longitude": "30.16469691"
      },
      "description": "Компания 'Терекс-Плюс' предоставляет в аренду дорожную и строительную технику по Санкт-Петербургу и Ленинградской области",
